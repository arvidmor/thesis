{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "${HOME}/tools/msp430-gcc/include/"
            ],
            "defines": [
                "__MSP430FR5994__"
            ],
            "compilerPath": "${HOME}/tools/msp430-gcc/bin/msp430-elf-gcc",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "${default}",
            "compilerArgs": [
                "-Isupport/include",
                "-mmcu=msp430fr5994"
            ],
            "browse": {
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}